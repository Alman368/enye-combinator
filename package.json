{
  "name": "enye-combinator",
  "version": "1.0.0",
  "description": "MindHealth Analytics - Mental Health Data Visualization Platform",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\" --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgGreen.bold\"",
    "frontend": "cd frontend && npm run dev",
    "backend": "cross-env-shell \"cd backend && (python run_dev.py || python3 run_dev.py)\"",
    "backend:init": "cd backend && (python init_db.py || python3 init_db.py)",
    "install": "npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && (pip install -r requirements.txt || pip3 install -r requirements.txt)",
    "dev": "npm start"
  },
  "keywords": [
    "mental-health",
    "analytics",
    "visualization",
    "fastapi",
    "react",
    "typescript"
  ],
  "author": "MindHealth Analytics Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0"
  }
}
